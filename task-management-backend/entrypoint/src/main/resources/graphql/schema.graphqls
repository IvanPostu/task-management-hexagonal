scalar Instant
scalar LocalDate
scalar Upload

type Query {
    getEmployee(id: ID) : Employee
    getAllStores: [Store]
}

type Mutation {
  uploadFile(files: [Upload!]!): String
}

type Store {
    id: ID!
    name: String
    city: String
    createDate: Instant!
    dueDate: LocalDate!
}

type Employee {
    id: ID!
    name: String!
    location: Store
}
