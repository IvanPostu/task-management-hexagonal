scalar Instant
scalar LocalDate

type Query {
    getEmployee(id: ID) : Employee
    getAllStores: [Store]
}


type FileInfo {
  filename: String!
  size: Int!
  contentType: String
}

type Store {
    id: ID!
    name: String
    city: String
    createDate: Instant!
    dueDate: LocalDate!
}

type Employee {
    id: ID!
    name: String!
    location: Store
}
